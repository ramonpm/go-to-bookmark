{
  "name": "Go To Bookmark",
  "version": "1.0",
  "description": "Chrome extension that only does 2 things: search inside your bookmarks and open the chosen one",
  "permissions": ["bookmarks"],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "24": "images/icon.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-p1WKSDRLazwpIo1hVSgse+vPtlzMta5e9+gKCpVCPLA='; object-src 'self'",
  "manifest_version": 2
}
