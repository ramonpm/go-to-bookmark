{
  "name": "Go To Bookmark",
  "version": "1.0",
  "description": "Find and open bookmarks or history items quicker than using the browser's address bar",
  "permissions": ["bookmarks", "history"],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Go To Bookmark"
  },
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F"
      }
    }
  },
  "content_security_policy": "script-src 'self' 'sha256-p1WKSDRLazwpIo1hVSgse+vPtlzMta5e9+gKCpVCPLA='; object-src 'self'",
  "manifest_version": 2
}
